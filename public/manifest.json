{
  "manifest_version": 2,
  "short_name": "Aiki",
  "name": "Aiki",
  "description": "Stop procrastinating and start learning!",
  "version": "3.0.0",
  "icons": {
    "128": "/images/aikido.png"
  },
  "offline_enabled": false,
  "browser_action": {
    "default_icon": "/images/aikido.png",
    "default_title": "Aiki",
    "default_popup": "/index.html"
  },
  "background": {
    "scripts": [
      "/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "idle",
    "alarms",
    "history",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "/images/aikido.png"
  ],
  "options_page": "/index.html?page=options"
}